  "private": true,
  "devDependencies": {
    "husky": "^8.0.2",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "turbo": "^1.6.3"
  },
  "scripts": {
    "dev": "npx turbo run dev",
    "build": "npx turbo run build",
    "start": "npx turbo run start",
